<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Number_8_Characters</fullName>
    <active>false</active>
    <description>Field needs to be 8 characters</description>
    <errorConditionFormula>LEN( AccountNumber) != 8</errorConditionFormula>
    <errorMessage>Please make sure all account numbers are 8 characters long</errorMessage>
</ValidationRule>
